<div class="block">
  <div class="content">            
    <h2 class="title"><%= t("web-app-theme.edit", :default => "Edit") %> Process</h2>
    <div class="inner">
      <%= form_for @process_definition, :url => process_path(@process_definition), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => { :f => f } %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>